{
  "name": "@1024pix/pix-ui-webcomponents",
  "version": "1.0.0",
  "description": "Ce repo a été créé pour le Tech Time 2023. Il s'agit de créer des webcomponents compatibles avec tous les Frameworks et au-delà utilisés chez Pix.",
  "module": "./dist/pix-ui.js",
  "main": "./dist/pix-ui.umd.cjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/pix-ui.js",
      "require": "./dist/pix-ui.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1024pix/pix-ui-webcomponents.git"
  },
  "author": "Pix",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/1024pix/pix-ui-webcomponents/issues"
  },
  "homepage": "https://github.com/1024pix/pix-ui-webcomponents#readme",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run lint",
    "lint": "eslint . --ext .vue,.js --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@1024pix/pix-ui-themes": "github:1024pix/pix-ui-themes#add-pluto-theme-design-tokens-file",
    "@rushstack/eslint-patch": "^1.3.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "prettier": "^2.8.8",
    "vite": "^4.3.9"
  }
}
